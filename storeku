<script>
  const menSection = document.getElementById('menSection');
  const womenSection = document.getElementById('womenSection');
  const kidsSection = document.getElementById('kidsSection');
  const sportSection = document.getElementById('sportSection');
  const brandSection = document.getElementById('brandSection');
  const outletSection = document.getElementById('outletSection');

  const navMen = document.getElementById('navMen');
  const navWomen = document.getElementById('navWomen');
  const navKids = document.getElementById('navKids');
  const navSport = document.getElementById('navSport');
  const navBrand = document.getElementById('navBrand');
  const navOutlet = document.getElementById('navOutlet');

  const navMenMobile = document.getElementById('navMenMobile');
  const navWomenMobile = document.getElementById('navWomenMobile');
  const navKidsMobile = document.getElementById('navKidsMobile');
  const navBrandMobile = document.getElementById('navKidsMobile');
  const navSportMobile = document.getElementById('navKidsMobile');
  const navOutletMobile = document.getElementById('navOutletMobile');
  const mobileMenu = document.getElementById('mobileMenu'); // pastikan ada elemen ini di HTML

  // Fungsi bantu untuk menyembunyikan semua section
  function hideAllSections() {
    menSection.classList.add('hidden');
    womenSection.classList.add('hidden');
    kidsSection.classList.add('hidden');
    sportSection.classList.add('hidden');
    brandSection.classList.add('hidden');
    outletSection.classList.add('hidden');
  }

  navMen.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    menSection.classList.remove('hidden');
  });

  navOutlet.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    outletSection.classList.remove('hidden');
  });
  

  navBrand.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    brandSection.classList.remove('hidden');
  });

  navWomen.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    womenSection.classList.remove('hidden');
  });

  navKids.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    kidsSection.classList.remove('hidden');
  });

  navSport.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    sportSection.classList.remove('hidden');
  });

  navBrand.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    brandSection.classList.remove('hidden');
  });

  // Mobile
  navMenMobile.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    menSection.classList.remove('hidden');
    mobileMenu.classList.add('hidden');
  });

  navSportMobile.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    spostSection.classList.remove('hidden');
    mobileMenu.classList.add('hidden');
  });

  navBrandomenMobile.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    brandSection.classList.remove('hidden');
    mobileMenu.classList.add('hidden');
  });


  navWomenMobile.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    womenSection.classList.remove('hidden');
    mobileMenu.classList.add('hidden');
  });

  navKidsMobile.addEventListener('click', e => {
    e.preventDefault();
    hideAllSections();
    kidsSection.classList.remove('hidden');
    mobileMenu.classList.add('hidden');
  });

  

  // Default: tampilkan menSection
  hideAllSections();
  menSection.classList.remove('hidden');
</script>
<script>
    const slider = document.getElementById('bannerSlider');
    const slides = slider.children;
    const totalSlides = slides.length;
    let currentIndex = 0;
  
    const prevBtn = document.getElementById('prevBtn');
    const nextBtn = document.getElementById('nextBtn');
    const dots = document.querySelectorAll('.dot');
  
    function updateSlider(index) {
      slider.style.transform = `translateX(-${index * 100}%)`;
      dots.forEach((dot, i) => {
        dot.classList.toggle('bg-gray-700', i === index);
        dot.classList.toggle('bg-gray-400', i !== index);
      });
    }
  
    prevBtn.addEventListener('click', () => {
      currentIndex = (currentIndex === 0) ? totalSlides - 1 : currentIndex - 1;
      updateSlider(currentIndex);
    });
  
    nextBtn.addEventListener('click', () => {
      currentIndex = (currentIndex + 1) % totalSlides;
      updateSlider(currentIndex);
    });
  
    dots.forEach((dot, i) => {
      dot.addEventListener('click', () => {
        currentIndex = i;
        updateSlider(currentIndex);
      });
    });
  
    setInterval(() => {
      currentIndex = (currentIndex + 1) % totalSlides;
      updateSlider(currentIndex);
    }, 5000);
  
    updateSlider(0);
    
  </script>

    
</body>


</html>
